{
  "name": "micro-rss-parser",
  "description": "Parse rss to json",
  "version": "1.1.1",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "dev": "micro-dev",
    "deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "micro",
    "rss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/micro-rss-parser.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-rss-parser/issues"
  },
  "homepage": "https://github.com/micro-rss-parser/micro-rss-parser#readme",
  "now": {
    "alias": [
      "parser.rss.micro.t-fk.no"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm",
    "engines": {
      "node": "8.9.4"
    }
  },
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.0",
    "micro-dev": "2.2.0",
    "nsp": "3.1.0",
    "nyc": "11.4.1",
    "standard": "10.0.3",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "axios": "0.17.1",
    "feedme": "1.1.2",
    "marked": "0.3.12",
    "micro": "9.1.0"
  }
}
